[user]
	name = Josh Shadlen
	email = josh.shadlen@gmail.com

[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = auto

[gc]
	auto=1

[merge]
	summary=true

[alias]
	cp = cherry-pick -x

[alias]
	b = branch -v
	r = remote -v
	t = tag -l

[alias]
	unstage = reset HEAD
	uncommit = reset --soft HEAD^

[core]
	editor = /usr/bin/vim
